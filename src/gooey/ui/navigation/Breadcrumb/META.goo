            "default": "/"
        },
        "separatoricon": {
            "type": "STRING",
            "default": null
        },
        "collapsible": {
            "type": "BOOLEAN",
            "default": false
        },
        "maxvisible": {
            "type": "NUMBER",
            "default": 0
        },
        "label": {
            "type": "STRING",
            "default": null
        },
        "height": {
            "type": "STRING",
            "default": null
        },
        "width": {
            "type": "STRING",
            "default": null
        },
        "tooltip": {
            "type": "STRING",
            "default": null
        },
        "visible": {
            "type": "BOOLEAN",
            "default": true
        },
        "disabled": {
            "type": "BOOLEAN",
            "default": false
        }
    },
    "tokens": {
