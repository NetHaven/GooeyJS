{
    "MetaGooVersion": "1.0",
    "name": "Breadcrumb",
    "prefix": "gooeyui",
    "tagName": "breadcrumb",
    "script": "Breadcrumb.js",
    "templates": [
        { "id": "ui-Breadcrumb", "file": "Breadcrumb.html" }
    ],
    "themes": {
        "default": "base",
