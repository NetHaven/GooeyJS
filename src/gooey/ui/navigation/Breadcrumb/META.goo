        "gooey-breadcrumb-text-color": { "description": "Breadcrumb text color", "fallback": "gooey-color-text-muted", "category": "color" },
        "gooey-breadcrumb-link-color": { "description": "Breadcrumb link color", "fallback": "gooey-color-text-link", "category": "color" },
        "gooey-breadcrumb-link-hover-color": { "description": "Breadcrumb link hover color", "fallback": null, "category": "color" },
        "gooey-breadcrumb-link-hover-bg": { "description": "Breadcrumb link hover background", "fallback": null, "category": "color" },
        "gooey-breadcrumb-link-active-bg": { "description": "Breadcrumb link active background", "fallback": null, "category": "color" },
        "gooey-breadcrumb-separator-color": { "description": "Breadcrumb separator color", "fallback": "gooey-color-text-disabled", "category": "color" },
