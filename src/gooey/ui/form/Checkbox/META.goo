{
  "MetaGooVersion": "1.0",
  "name": "Checkbox",
  "prefix": "gooeyui",
  "tagName": "checkbox",
  "script": "Checkbox.js",
  "templates": [
    { "id": "ui-Checkbox", "file": "Checkbox.html" }
  ],
  "themes": {
    "default": "base",
    "available": ["base", "classic", "janus"]
  },
  "tokens": {
    "gooey-checkbox-focus-ring-color": {
      "description": "Checkbox keyboard focus ring color",
      "fallback": "gooey-color-focus",
      "category": "color"
    },
    "gooey-checkbox-focus-ring-width": {
      "description": "Checkbox focus ring width",
      "fallback": "gooey-focus-width",
      "category": "border"
    },
    "gooey-checkbox-opacity-disabled": {
      "description": "Checkbox disabled opacity",
      "fallback": "gooey-opacity-disabled",
      "category": "opacity"
    }
  },
  "attributes": {
    "draggable": { "type": "BOOLEAN", "default": false },
    "droppable": { "type": "BOOLEAN", "default": false },
    "dropzone": { "type": "STRING", "default": null },
    "height": { "type": "STRING", "default": null },
    "width": { "type": "STRING", "default": null },
    "tooltip": { "type": "STRING", "default": null },
    "visible": { "type": "BOOLEAN", "default": true },
    "disabled": { "type": "BOOLEAN", "default": false },
    "required": { "type": "BOOLEAN", "default": false }
  }
}
